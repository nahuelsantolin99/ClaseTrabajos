#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>
#include "utn.h"

int main(void)
{
	setbuf(stdout,NULL);

	char cadenaAlfanumerica[20];

	if(getAlfanumerico(cadenaAlfanumerica, sizeof(cadenaAlfanumerica)) == 0)
	{
		printf("El alfanumerico es: %s", cadenaAlfanumerica);
	}
	else
	{
		printf("No se pudo conseguir el alfanumerico");
	}

	return EXIT_SUCCESS;
}





