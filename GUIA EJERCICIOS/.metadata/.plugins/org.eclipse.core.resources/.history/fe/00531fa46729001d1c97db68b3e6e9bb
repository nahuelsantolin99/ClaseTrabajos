#include <stdio.h>
#include <stdlib.h>

void mostrarEntero(int numeroEntero);

int main(void)
{
	int numero;

	printf("Ingrese un numero: ");
	scanf("%d", &numero);

	mostrarEntero(numero);

	return EXIT_SUCCESS;
}

void mostrarEntero(int numeroEntero)
{
	printf("Ingreso %d", numeroEntero);
}
